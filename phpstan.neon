includes:
  - vendor/larastan/larastan/extension.neon
  - vendor/phpstan/phpstan/conf/bleedingEdge.neon

parameters:
  level: 5
  ignoreErrors:
    -
      identifier: larastan.noEnvCallsOutsideOfConfig
      path: **/config/**

  paths:
    - app
    - bootstrap
    - database/factories
    - routes

  excludePaths:
    - vendor/
